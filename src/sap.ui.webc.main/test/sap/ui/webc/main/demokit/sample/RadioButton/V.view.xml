<mvc:View controllerName="sap.ui.webc.main.sample.RadioButton.C" xmlns="sap.ui.webc.main" xmlns:fiori="sap.ui.webc.fiori" xmlns:mvc="sap.ui.core.mvc" height="100%">
    <Toast id="demoToast" duration="1000"></Toast>

    <fiori:Page>
        <fiori:header>
            <fiori:Bar>
                <fiori:middleContent>
                    <Title text="RadioButton"></Title>
                </fiori:middleContent>
            </fiori:Bar>
        </fiori:header>

        <fiori:content>
            <RadioButton text="Some text..." value="Control value" valueState="Warning" change="handleChange"></RadioButton>
        </fiori:content>
    </fiori:Page>
</mvc:View>