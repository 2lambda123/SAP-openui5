<mvc:View controllerName="sap.ui.webc.fiori.sample.BarcodeScannerDialog.C" xmlns="sap.ui.webc.main" xmlns:fiori="sap.ui.webc.fiori" xmlns:mvc="sap.ui.core.mvc" height="100%">
    <Toast id="demoToast" duration="1000"></Toast>

    <fiori:Page>
        <fiori:header>
            <fiori:Bar>
                <fiori:middleContent>
                    <Title text="BarcodeScannerDialog"></Title>
                </fiori:middleContent>
            </fiori:Bar>
        </fiori:header>

        <fiori:content>
            <fiori:BarcodeScannerDialog scanError="handleScanError" scanSuccess="handleScanSuccess"></fiori:BarcodeScannerDialog>
        </fiori:content>
    </fiori:Page>
</mvc:View>