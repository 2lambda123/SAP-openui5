<mvc:View xmlns="sap.f" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" height="100%">
	<FlexibleColumnLayout layout="ThreeColumnsBeginExpandedEndHidden">
		<landmarkInfo>
			<FlexibleColumnLayoutAccessibleLandmarkInfo
					firstColumnLabel="Custom Label For First Column"
					lastColumnLabel="Test Label for Last Column"/>
		</landmarkInfo>
		<beginColumnPages>
			<mvc:XMLView viewName="sap.f.sample.FlexibleColumnLayoutLandmarkInfo.view.Master"/>
		</beginColumnPages>
		<midColumnPages>
			<mvc:XMLView viewName="sap.f.sample.FlexibleColumnLayoutLandmarkInfo.view.Detail"/>
		</midColumnPages>
		<endColumnPages>
			<mvc:XMLView viewName="sap.f.sample.FlexibleColumnLayoutLandmarkInfo.view.DetailDetail"/>
		</endColumnPages>
	</FlexibleColumnLayout>
</mvc:View>
