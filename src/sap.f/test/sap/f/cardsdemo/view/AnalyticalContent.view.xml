<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">

	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer />
				<Title text="Analytical Content Card" />
				<ToolbarSpacer />
			</Toolbar>
		</subHeader>

		<Panel>
			<w:Card
				id="line"
				manifest="{analyticalContentManifests>/line}"
				baseUrl="./"
				class="sapUiMediumMargin"
				width="600px" />
			<w:Card
				id="stackedBar"
				manifest="{analyticalContentManifests>/stackedColumn}"
				baseUrl="./"
				class="sapUiMediumMargin"
				height="500px"
				width="600px" />
			<w:Card
				id="stackedColumn"
				manifest="{analyticalContentManifests>/stackedBar}"
				baseUrl="./"
				class="sapUiMediumMargin"
				height="500px"
				width="600px" />
			<w:Card
				id="donut"
				manifest="{analyticalContentManifests>/donut}"
				baseUrl="./"
				class="sapUiMediumMargin"
				height="500px"
				width="600px" />
			<w:Card
				manifest="{analyticalContentManifests>/heatmap}"
				baseUrl="./"
				class="sapUiMediumMargin"
				height="500px"
				width="600px" />
			<w:Card
				id="bubble"
				manifest="{analyticalContentManifests>/bubble}"
				baseUrl="./"
				class="sapUiMediumMargin"
				height="500px"
				width="600px" />
		</Panel>
	</Page>
</mvc:View>