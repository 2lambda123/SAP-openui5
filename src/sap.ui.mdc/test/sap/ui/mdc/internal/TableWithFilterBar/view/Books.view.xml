<mvc:View id="booksView" displayBlock="true"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:f2="sap.f"
	xmlns:mdc="sap.ui.mdc"
	xmlns:mdcat="sap.ui.mdc.actiontoolbar"
	xmlns:mdcc="sap.ui.mdc.chart"
	xmlns:mdccn="sap.ui.mdc.chartNew"
	xmlns:mdcf="sap.ui.mdc.field"
	xmlns:mdcl="sap.ui.mdc.link"
	xmlns:ui="sap.ui"
	xmlns:variants="sap.ui.fl.variants"
	xmlns="sap.m"
	xmlns:types="sap.ui.v4demo.types"
	xmlns:vhfb="sap.ui.mdc.filterbar.vh"
	xmlns:mdcv="sap.ui.mdc.valuehelp"
	xmlns:mdcvc="sap.ui.mdc.valuehelp.content"
	xmlns:demo="sap.ui.v4demo.controls" controllerName="sap.ui.v4demo.controller.Books">
		<f2:DynamicPage id="dynamicPage">
			<f2:title>
				<f2:DynamicPageTitle>
					<f2:actions>
<!--
						<Button id="changeProperty1" text="Change Req Property" press="onChangeReqProperty" />
						<Button id="changeProperty2" text="Change Vis Property" press="onChangeVisProperty" />
 -->
					</f2:actions>
					<f2:heading>
						<variants:VariantManagement id="IDVariantManagementOfTable" for="dynamicPage" class="sapUiTinyMarginBottom"/>
					</f2:heading>

					<f2:snappedContent>
						<Text id="statusTextCollapsed" />
					</f2:snappedContent>
					<f2:expandedContent>
						<Text id="statusTextExpanded" />
					</f2:expandedContent>
				</f2:DynamicPageTitle>
			</f2:title>
			<f2:header>
				<f2:DynamicPageHeader>
					<f2:content>
						<mdc:FilterBar id="booksFilterBar" variantBackreference="IDVariantManagementOfTable" filtersChanged="onFiltersChanged" filterConditions = '\{"stock" : [\{"operator":"BT","values":[100,50000]}]\}' liveMode="false" delegate="{name: 'sap/ui/v4demo/delegate/Books.FB.delegate', payload: {collectionName: 'Books'}}" p13nMode="Item,Value">
						<mdc:dependents>
							<mdc:ValueHelp
								id="fhAdob"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:dialog>
									<mdcv:Popover title="Birth Epoch">
										<mdcvc:Conditions label="Date of birth"></mdcvc:Conditions>
									</mdcv:Popover>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp
								id="fhAdobDialog"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:dialog>
									<mdcv:Dialog title="Birth Epoch">
										<mdcvc:Conditions label="Date of birth"></mdcvc:Conditions>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp
								id="fhAdod"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:dialog>
									<mdcv:Popover title="Date of Death">
										<mdcvc:Conditions label="Date of death"></mdcvc:Conditions>
									</mdcv:Popover>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp
								id="fhAdodDialog"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:dialog>
									<mdcv:Dialog title="Date of Death">
										<mdcvc:Conditions label="Date of death"></mdcvc:Conditions>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>


							<mdc:ValueHelp id="FH4"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:typeahead>
									<mdcv:Popover title="Title">
										<mdcvc:MTable keyPath="title" descriptionPath="title" filterFields="$search">
											<Table id="mTable1_title" items='{path : "/Books", length: 10}' width="30rem">
												<columns>
													<Column>
														<header>
															<Text text="Title" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{title}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
									</mdcv:Popover>
								</mdc:typeahead>
								<mdc:dialog>
									<mdcv:Dialog title="Title">
										<mdcvc:MDCTable keyPath="title" descriptionPath="title" filterFields="$search">
											<mdcvc:filterBar>
												<vhfb:FilterBar liveMode="false" delegate="{name: 'sap/ui/mdc/filterbar/vh/GenericFilterBarDelegate', payload: {}}">
													<vhfb:basicSearchField>
														<mdc:FilterField
															delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}"
															dataType= "Edm.String"
															conditions= "{$filters>/conditions/$search}"
															width="50%"
															maxConditions="1"
															placeholder="Search" />
													</vhfb:basicSearchField>
													<vhfb:filterItems>
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="Title" conditions="{$filters>/conditions/title}" />
													</vhfb:filterItems>
												</vhfb:FilterBar>
											</mdcvc:filterBar>
											<mdc:Table id="FH4--mdc--gridtable"
												autoBindOnInit="true"
												width="100%"
												height="100%"
												selectionMode="{= ${settings>/maxConditions} === -1 ? 'Multi' : 'Single'}"
												p13nMode="Column,Sort"
												delegate="{name: 'sap/ui/v4demo/delegate/GridTable.delegate', payload: {collectionName: 'Books'}}" threshold="50">
												<mdc:columns>
													<mdc:table.Column id="FH4--mdc--gridtable--id" header="ID" dataProperty="ID" width="5rem">
														<mdc:Field delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" value="{ID}" editMode="Display" width="100%"/>
													</mdc:table.Column>
													<mdc:table.Column id="FH4--mdc--gridtable--title" header="Title" dataProperty="title">
														<mdc:Field delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" value="{title}" editMode="Display" width="100%"/>
													</mdc:table.Column>
													<mdc:table.Column id="FH4--mdc--gridtable--descr" header="Description" dataProperty="descr">
														<mdc:Field delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" value="{descr}" editMode="Display" width="100%"/>
													</mdc:table.Column>
													<mdc:table.Column id="FH4--mdc--gridtable--pub" header="Published" dataProperty="published">
														<mdc:Field delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" value="{published}" editMode="Display" width="100%"/>
													</mdc:table.Column>
													<mdc:table.Column id="FH4--mdc--gridtable--genre" header="Genre" dataProperty="genre_code">
														<mdc:Field delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" value="{genre_code}" additionalValue="{genre/label}" editMode="Display" width="100%"/>
													</mdc:table.Column>
												</mdc:columns>
												<mdc:type>
													<mdc:table.GridTableType rowCountMode="Auto"/>
												</mdc:type>
											</mdc:Table>
										</mdcvc:MDCTable>
										<mdcvc:Conditions label="Title"/>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp id="FH1"
								delegate="{name: 'sap/ui/v4demo/delegate/Books.AuthorVH.delegate', payload: {}}">
								<mdc:typeahead>
									<mdcv:Popover id="FH1-Popover" title="Author ID">
										<mdcvc:MTable id="FH1-Popover-MTable" keyPath="ID" descriptionPath="name" filterFields="$search">
											<Table id="mTable1" items='{path : "/Authors", length: 10}' width="30rem">
												<columns>
													<Column width="5rem">
														<header>
															<Text text="ID" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Name" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{path: 'ID', type:'sap.ui.model.odata.type.Int32', formatOptions: {groupingEnabled: false}}" />
															<Text text="{name}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
									</mdcv:Popover>
								</mdc:typeahead>
								<mdc:dialog>
									<mdcv:Dialog id="FH1-Dialog" title="Author ID">
										<mdcvc:MTable id="FH1-Dialog-MTable" keyPath="ID" descriptionPath="name" filterFields="$search">
											<mdcvc:collectiveSearchItems>
												<core:Item text="Default" key="default"/>
												<core:Item text="with Country" key="template1"/>
											</mdcvc:collectiveSearchItems>

											<mdcvc:filterBar>
												<vhfb:FilterBar liveMode="false" delegate="{name: 'sap/ui/mdc/filterbar/vh/GenericFilterBarDelegate', payload: {}}">
													<vhfb:basicSearchField>
														<mdc:FilterField
															delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}"
															dataType= "Edm.String"
															conditions= "{$filters>/conditions/$search}"
															width="50%"
															maxConditions="1"
															placeholder="Search" />
													</vhfb:basicSearchField>
													<vhfb:filterItems>
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="ID" dataType= "Edm.Int32" dataTypeFormatOptions="{groupingEnabled: false}" conditions= "{$filters>/conditions/ID}" />
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="Name" conditions= "{$filters>/conditions/name}" />
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="Date of Birth" dataType= "Edm.Date" maxConditions="-1" conditions= "{$filters>/conditions/dateOfBirth}" fieldHelp= "FH2">
															<mdc:dependents>
																<mdc:ValueHelp id="FH2"
																	delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
																	<mdc:dialog>
																		<mdcv:Dialog title="Date of Birth">
																			<mdcvc:Conditions label="Date of Birth"/>
																		</mdcv:Dialog>
																	</mdc:dialog>
																</mdc:ValueHelp>
															</mdc:dependents>
														</mdc:FilterField>
													</vhfb:filterItems>
												</vhfb:FilterBar>
											</mdcvc:filterBar>
											<Table id="mTable2" items='{path : "/Authors"}' width="100%" growing="true" growingScrollToLoad="true" growingThreshold="20">
												<columns>
													<Column width="5rem">
														<header>
															<Text text="ID" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Name" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Date of birth" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text id="mTable2-authorId" text="{path: 'ID', type:'sap.ui.model.odata.type.Int32', formatOptions: {groupingEnabled: false}}" />
															<Text text="{name}" />
															<Text text="{dateOfBirth}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
										<mdcvc:Conditions label="Author ID"/>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp id="FHPublished"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:dialog>
									<mdcv:Dialog title="Published">
										<mdcvc:Conditions label="Published"/>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp id="FHLanguage"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:typeahead>
									<mdcv:Popover title="Langugages">
										<mdcvc:MTable keyPath="code" descriptionPath="name" filterFields="$search">
											<Table id="mTable1_languages" items='{path : "/Languages", length: 10}' width="30rem">
												<columns>
													<Column width="5rem">
														<header>
															<Text text="Code" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Name" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{code}" />
															<Text text="{name}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
									</mdcv:Popover>
								</mdc:typeahead>
								<mdc:dialog>
									<mdcv:Dialog title="Languages">
										<mdcvc:MTable keyPath="code" descriptionPath="name" filterFields="$search">
											<Table id="mTable2_languages" items='{path : "/Languages"}' width="100%" growing="true" growingScrollToLoad="true" growingThreshold="20">
												<columns>
													<Column width="5rem">
														<header>
															<Text text="Code" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Name" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="navitve Name" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{code}" />
															<Text text="{name}" />
															<Text text="{nativeName}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
										<mdcvc:Conditions label="Languages"/>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp id="FHClassification"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:typeahead>
									<mdcv:Popover title="Classification">
										<mdcvc:MTable keyPath="code" descriptionPath="title" filterFields="$search">
											<Table id="mTable1_classification" items='{path : "/Classification", length: 10}' width="30rem">
												<columns>
													<Column width="5rem">
														<header>
															<Text text="Code" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Title" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{code}" />
															<Text text="{title}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
									</mdcv:Popover>
								</mdc:typeahead>
								<mdc:dialog>
									<mdcv:Dialog title="Classification">
										<mdcvc:MTable keyPath="code" descriptionPath="title" filterFields="$search">
											<Table id="mTable2_classification" items='{path : "/Classification"}' width="100%" growing="true" growingScrollToLoad="true" growingThreshold="20">
												<columns>
													<Column width="5rem">
														<header>
															<Text text="Code" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Title" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{code}" />
															<Text text="{title}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
										<mdcvc:Conditions label="Classification"/>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp id="FHSubGenre"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:inParameters>
									<mdcf:InParameter id="IOFFVHSub-In-Class" value="{$filters>/conditions/classification_code}" helpPath="classification_code" />
									<mdcf:InParameter id="IOFFVHSub-In-Genre" value="{$filters>/conditions/genre_code}" helpPath="genre_code" />
								</mdc:inParameters>
								<mdc:outParameters>
									<mdcf:OutParameter id="IOFFVHSub-Out-Class" value="{$filters>/conditions/classification_code}" helpPath="classification_code" />
									<mdcf:OutParameter id="IOFFVHSub-Out-Genre" value="{$filters>/conditions/genre_code}" helpPath="genre_code" />
								</mdc:outParameters>
								<mdc:typeahead>
									<mdcv:Popover title="Sub Genre">
										<mdcvc:MTable keyPath="code" descriptionPath="title" filterFields="$search">
											<Table id="mTable1_subgenre" items='{path : "/SubGenres", length: 10}' width="30rem">
												<columns>
													<Column width="10rem">
														<header>
															<Text text="Code" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Title" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Genre" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Classification" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{code}" />
															<Text text="{title}" />
															<Text text="{genre_code}" />
															<Text text="{classification_code}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
									</mdcv:Popover>
								</mdc:typeahead>
								<mdc:dialog>
									<mdcv:Dialog title="Sub Genre">
										<mdcvc:MTable keyPath="code" descriptionPath="title" filterFields="$search">
											<mdcvc:filterBar>
												<vhfb:FilterBar liveMode="false" delegate="{name: 'sap/ui/mdc/filterbar/vh/GenericFilterBarDelegate', payload: {collectionName: ''}}" >
													<vhfb:basicSearchField>
														<mdc:FilterField
															delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}"
															dataType= "Edm.String"
															conditions= "{$filters>/conditions/$search}"
															width="50%"
															maxConditions="1"
															placeholder="Search" />
													</vhfb:basicSearchField>
													<vhfb:filterItems>
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="Code" dataType= "Edm.String" conditions= "{$filters>/conditions/code}" />
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="Title" conditions= "{$filters>/conditions/title}" />
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="Classification" dataType= "Edm.String" conditions= "{$filters>/conditions/classification_code}" />
														<mdc:FilterField delegate="{name: 'sap/ui/mdc/odata/v4/FieldBaseDelegate', payload: {}}" label="Genre" dataType= "Edm.String" conditions= "{$filters>/conditions/genre_code}" />
													</vhfb:filterItems>
												</vhfb:FilterBar>
											</mdcvc:filterBar>
											<Table id="mTable2_subgenre" items='{path : "/SubGenres"}' width="100%" growing="true" growingScrollToLoad="true" growingThreshold="20">
												<columns>
													<Column width="10rem">
														<header>
															<Text text="Code" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Title" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Genre" />
														</header>
													</Column>
													<Column>
														<header>
															<Text text="Classification" />
														</header>
													</Column>
												</columns>
												<items>
													<ColumnListItem type="Active">
														<cells>
															<Text text="{code}" />
															<Text text="{title}" />
															<Text text="{genre_code}" />
															<Text text="{classification_code}" />
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</mdcvc:MTable>
										<mdcvc:Conditions label="Sub Genre"/>
									</mdcv:Dialog>
								</mdc:dialog>
							</mdc:ValueHelp>

							<mdc:ValueHelp id="FHDetailGenre"
								delegate="{name: 'sap/ui/mdc/odata/v4/ValueHelpDelegate', payload: {}}">
								<mdc:inParameters>
									<mdcf:InParameter id="IOFFVHDetail-In-Class" value="{$filters>/conditions/classification_code}" helpPath="classification_code" />
									<mdcf:InParameter id="IOFFVHDetail-In-Genre" value="{$filters>/conditions/genre_code}" helpPath="genre_code" />
									<mdcf:InParameter id="IOFFVHDetail-In-SubGenre" value="{$filters>/conditions/subgenre_code}" helpPath="subgenre_code" />
								</mdc:inParameters>
								<mdc:outParameters>
									<mdcf:OutParameter id="IOFFVHDetail-Out-Class" value="{$filters>/conditions/classification_code}" helpPath="classification_code" />
									<mdcf:OutParameter id="IOFFVHDetail-Out-Genre" value="{$filters>/conditions/genre_code}" helpPath="genre_code" />
									<mdcf:OutParameter id="IOFFVHDetail-Out-SubGenre" value="{$filters>/conditions/subgenre_code}" helpPath="subgenre_code" />
								</mdc:outParameters>
								<mdc:typeahead>
									<mdcv:Popover title="Detail Genre">
										<mdcvc:MTable keyPath="code" descriptionPath="title" filterFields="$search">
											<Table id="mTable1_detailgenre" items='{path : "/DetailGenres", length: 10}' width="30rem">
												<columns>
													<Column width="10rem">
														<header>
															<Text text="Code" />
														</header>
													</Column>
													<Column>
